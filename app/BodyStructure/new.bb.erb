<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New BodyStructure</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @bodystructure.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="bodystructure[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Syphilis: </td>
                <td class="itemValue"><input type="text" name="bodystructure[syphilis]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Hernia: </td>
                <td class="itemValue"><input type="text" name="bodystructure[hernia]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Goitre: </td>
                <td class="itemValue"><input type="text" name="bodystructure[goitre]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Filariasis: </td>
                <td class="itemValue"><input type="text" name="bodystructure[filariasis]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Varicoseveins: </td>
                <td class="itemValue"><input type="text" name="bodystructure[varicoseveins]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Skineruptions: </td>
                <td class="itemValue"><input type="text" name="bodystructure[skineruptions]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Gonorrhea: </td>
                <td class="itemValue"><input type="text" name="bodystructure[gonorrhea]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
