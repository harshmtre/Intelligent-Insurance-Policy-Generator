<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @dentalissues.id %></h1>
    <a href="<%= url_for :action => :show, :id => @dentalissues.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @dentalissues.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @dentalissues.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="dentalissues[id]" class="fieldLabel">Id</label>
            <input type="text" id="dentalissues[id]" name="dentalissues[id]" value="<%= @dentalissues.id %>" <%= placeholder( "Id" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="dentalissues[pyrarrhoea]" class="fieldLabel">Pyrarrhoea</label>
            <input type="text" id="dentalissues[pyrarrhoea]" name="dentalissues[pyrarrhoea]" value="<%= @dentalissues.pyrarrhoea %>" <%= placeholder( "Pyrarrhoea" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="dentalissues[missingteeth]" class="fieldLabel">Missingteeth</label>
            <input type="text" id="dentalissues[missingteeth]" name="dentalissues[missingteeth]" value="<%= @dentalissues.missingteeth %>" <%= placeholder( "Missingteeth" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
