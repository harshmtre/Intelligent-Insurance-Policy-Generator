<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @dentalissues.object %>
    <h1>Edit <%= @dentalissues.id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @dentalissues.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @dentalissues.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id: </td>
              <td class="itemValue"><input type="text" name="dentalissues[id]" value="<%= @dentalissues.id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Pyrarrhoea: </td>
              <td class="itemValue"><input type="text" name="dentalissues[pyrarrhoea]" value="<%= @dentalissues.pyrarrhoea %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Missingteeth: </td>
              <td class="itemValue"><input type="text" name="dentalissues[missingteeth]" value="<%= @dentalissues.missingteeth %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
