<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @famliyhistory.Id %></h1>
    <a href="<%= url_for :action => :show, :id => @famliyhistory.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @famliyhistory.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @famliyhistory.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="famliyhistory[Id]" class="fieldLabel">Id</label>
            <input type="text" id="famliyhistory[Id]" name="famliyhistory[Id]" value="<%= @famliyhistory.Id %>" <%= placeholder( "Id" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="famliyhistory[Diabetes]" class="fieldLabel">Diabetes</label>
            <input type="text" id="famliyhistory[Diabetes]" name="famliyhistory[Diabetes]" value="<%= @famliyhistory.Diabetes %>" <%= placeholder( "Diabetes" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="famliyhistory[Cancer]" class="fieldLabel">Cancer</label>
            <input type="text" id="famliyhistory[Cancer]" name="famliyhistory[Cancer]" value="<%= @famliyhistory.Cancer %>" <%= placeholder( "Cancer" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="famliyhistory[Infec_Diseases]" class="fieldLabel">Infec diseases</label>
            <input type="text" id="famliyhistory[Infec_Diseases]" name="famliyhistory[Infec_Diseases]" value="<%= @famliyhistory.Infec_Diseases %>" <%= placeholder( "Infec diseases" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
