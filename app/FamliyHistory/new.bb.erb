<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New FamliyHistory</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @famliyhistory.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="famliyhistory[Id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Diabetes: </td>
                <td class="itemValue"><input type="text" name="famliyhistory[Diabetes]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Cancer: </td>
                <td class="itemValue"><input type="text" name="famliyhistory[Cancer]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Infec diseases: </td>
                <td class="itemValue"><input type="text" name="famliyhistory[Infec_Diseases]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
