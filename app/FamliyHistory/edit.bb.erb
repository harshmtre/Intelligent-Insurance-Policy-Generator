<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @famliyhistory.object %>
    <h1>Edit <%= @famliyhistory.Id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @famliyhistory.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @famliyhistory.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id: </td>
              <td class="itemValue"><input type="text" name="famliyhistory[Id]" value="<%= @famliyhistory.Id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Diabetes: </td>
              <td class="itemValue"><input type="text" name="famliyhistory[Diabetes]" value="<%= @famliyhistory.Diabetes %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Cancer: </td>
              <td class="itemValue"><input type="text" name="famliyhistory[Cancer]" value="<%= @famliyhistory.Cancer %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Infec diseases: </td>
              <td class="itemValue"><input type="text" name="famliyhistory[Infec_Diseases]" value="<%= @famliyhistory.Infec_Diseases %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
