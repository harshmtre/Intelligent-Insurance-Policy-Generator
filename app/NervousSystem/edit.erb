<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @nervoussystem.id %></h1>
    <a href="<%= url_for :action => :show, :id => @nervoussystem.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @nervoussystem.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @nervoussystem.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[id]" class="fieldLabel">Id</label>
            <input type="text" id="nervoussystem[id]" name="nervoussystem[id]" value="<%= @nervoussystem.id %>" <%= placeholder( "Id" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[epilepsy]" class="fieldLabel">Epilepsy</label>
            <input type="text" id="nervoussystem[epilepsy]" name="nervoussystem[epilepsy]" value="<%= @nervoussystem.epilepsy %>" <%= placeholder( "Epilepsy" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[paralysis]" class="fieldLabel">Paralysis</label>
            <input type="text" id="nervoussystem[paralysis]" name="nervoussystem[paralysis]" value="<%= @nervoussystem.paralysis %>" <%= placeholder( "Paralysis" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[insanity]" class="fieldLabel">Insanity</label>
            <input type="text" id="nervoussystem[insanity]" name="nervoussystem[insanity]" value="<%= @nervoussystem.insanity %>" <%= placeholder( "Insanity" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[nervousbreakdown]" class="fieldLabel">Nervousbreakdown</label>
            <input type="text" id="nervoussystem[nervousbreakdown]" name="nervoussystem[nervousbreakdown]" value="<%= @nervoussystem.nervousbreakdown %>" <%= placeholder( "Nervousbreakdown" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[alzheimer]" class="fieldLabel">Alzheimer</label>
            <input type="text" id="nervoussystem[alzheimer]" name="nervoussystem[alzheimer]" value="<%= @nervoussystem.alzheimer %>" <%= placeholder( "Alzheimer" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[multiplesclerosis]" class="fieldLabel">Multiplesclerosis</label>
            <input type="text" id="nervoussystem[multiplesclerosis]" name="nervoussystem[multiplesclerosis]" value="<%= @nervoussystem.multiplesclerosis %>" <%= placeholder( "Multiplesclerosis" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="nervoussystem[parkinson]" class="fieldLabel">Parkinson</label>
            <input type="text" id="nervoussystem[parkinson]" name="nervoussystem[parkinson]" value="<%= @nervoussystem.parkinson %>" <%= placeholder( "Parkinson" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
