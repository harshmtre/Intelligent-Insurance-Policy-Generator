<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New GeneralHealth</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @generalhealth.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Email: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Email]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Army navy: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Army_Navy]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Height: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Height]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Weight: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Weight]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Doc: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Doc]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Prev surgeries: </td>
                <td class="itemValue"><input type="text" name="generalhealth[Prev_Surgeries]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
