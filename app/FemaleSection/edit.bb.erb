<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @femalesection.object %>
    <h1>Edit <%= @femalesection.Id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @femalesection.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @femalesection.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id: </td>
              <td class="itemValue"><input type="text" name="femalesection[Id]" value="<%= @femalesection.Id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Pregnant: </td>
              <td class="itemValue"><input type="text" name="femalesection[Pregnant]" value="<%= @femalesection.Pregnant %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Last delivery: </td>
              <td class="itemValue"><input type="text" name="femalesection[Last_Delivery]" value="<%= @femalesection.Last_Delivery %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Abortion: </td>
              <td class="itemValue"><input type="text" name="femalesection[Abortion]" value="<%= @femalesection.Abortion %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Ump: </td>
              <td class="itemValue"><input type="text" name="femalesection[UMP]" value="<%= @femalesection.UMP %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Rmp: </td>
              <td class="itemValue"><input type="text" name="femalesection[RMP]" value="<%= @femalesection.RMP %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Complications: </td>
              <td class="itemValue"><input type="text" name="femalesection[Complications]" value="<%= @femalesection.Complications %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Weakness: </td>
              <td class="itemValue"><input type="text" name="femalesection[Weakness]" value="<%= @femalesection.Weakness %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
