<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @excretarysystem.object %>
    <h1>Edit <%= @excretarysystem.id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @excretarysystem.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @excretarysystem.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id: </td>
              <td class="itemValue"><input type="text" name="excretarysystem[id]" value="<%= @excretarysystem.id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Chronic: </td>
              <td class="itemValue"><input type="text" name="excretarysystem[chronic]" value="<%= @excretarysystem.chronic %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Prostratediseases: </td>
              <td class="itemValue"><input type="text" name="excretarysystem[prostratediseases]" value="<%= @excretarysystem.prostratediseases %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Urinary: </td>
              <td class="itemValue"><input type="text" name="excretarysystem[urinary]" value="<%= @excretarysystem.urinary %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Kidneystone: </td>
              <td class="itemValue"><input type="text" name="excretarysystem[kidneystone]" value="<%= @excretarysystem.kidneystone %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
