<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New ExcretarySystem</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @excretarysystem.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="excretarysystem[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Chronic: </td>
                <td class="itemValue"><input type="text" name="excretarysystem[chronic]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Prostratediseases: </td>
                <td class="itemValue"><input type="text" name="excretarysystem[prostratediseases]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Urinary: </td>
                <td class="itemValue"><input type="text" name="excretarysystem[urinary]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Kidneystone: </td>
                <td class="itemValue"><input type="text" name="excretarysystem[kidneystone]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
