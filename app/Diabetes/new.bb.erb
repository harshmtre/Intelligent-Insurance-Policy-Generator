<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Diabetes</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @diabetes.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="diabetes[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Type1: </td>
                <td class="itemValue"><input type="text" name="diabetes[type1]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Typ2: </td>
                <td class="itemValue"><input type="text" name="diabetes[typ2]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Gestationaldiabetes: </td>
                <td class="itemValue"><input type="text" name="diabetes[gestationaldiabetes]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
