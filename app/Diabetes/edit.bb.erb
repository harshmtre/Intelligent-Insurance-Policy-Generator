<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @diabetes.object %>
    <h1>Edit <%= @diabetes.id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @diabetes.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @diabetes.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Id: </td>
              <td class="itemValue"><input type="text" name="diabetes[id]" value="<%= @diabetes.id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Type1: </td>
              <td class="itemValue"><input type="text" name="diabetes[type1]" value="<%= @diabetes.type1 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Typ2: </td>
              <td class="itemValue"><input type="text" name="diabetes[typ2]" value="<%= @diabetes.typ2 %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Gestationaldiabetes: </td>
              <td class="itemValue"><input type="text" name="diabetes[gestationaldiabetes]" value="<%= @diabetes.gestationaldiabetes %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
