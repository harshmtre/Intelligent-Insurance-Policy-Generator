<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New FinalDisplay</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @finaldisplay.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="finaldisplay[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Value: </td>
                <td class="itemValue"><input type="text" name="finaldisplay[value]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Risk: </td>
                <td class="itemValue"><input type="text" name="finaldisplay[risk]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Premium: </td>
                <td class="itemValue"><input type="text" name="finaldisplay[premium]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
