<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New OtherDiseases</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @otherdiseases.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Id: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Cancer: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[cancer]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Hiv: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[hiv]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Leprosy: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[leprosy]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Rheumatism: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[rheumatism]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Tumor: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[tumor]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Gout: </td>
                <td class="itemValue"><input type="text" name="otherdiseases[gout]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
