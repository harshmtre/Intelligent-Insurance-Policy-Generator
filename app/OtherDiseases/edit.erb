<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @otherdiseases.id %></h1>
    <a href="<%= url_for :action => :show, :id => @otherdiseases.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @otherdiseases.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @otherdiseases.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[id]" class="fieldLabel">Id</label>
            <input type="text" id="otherdiseases[id]" name="otherdiseases[id]" value="<%= @otherdiseases.id %>" <%= placeholder( "Id" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[cancer]" class="fieldLabel">Cancer</label>
            <input type="text" id="otherdiseases[cancer]" name="otherdiseases[cancer]" value="<%= @otherdiseases.cancer %>" <%= placeholder( "Cancer" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[hiv]" class="fieldLabel">Hiv</label>
            <input type="text" id="otherdiseases[hiv]" name="otherdiseases[hiv]" value="<%= @otherdiseases.hiv %>" <%= placeholder( "Hiv" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[leprosy]" class="fieldLabel">Leprosy</label>
            <input type="text" id="otherdiseases[leprosy]" name="otherdiseases[leprosy]" value="<%= @otherdiseases.leprosy %>" <%= placeholder( "Leprosy" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[rheumatism]" class="fieldLabel">Rheumatism</label>
            <input type="text" id="otherdiseases[rheumatism]" name="otherdiseases[rheumatism]" value="<%= @otherdiseases.rheumatism %>" <%= placeholder( "Rheumatism" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[tumor]" class="fieldLabel">Tumor</label>
            <input type="text" id="otherdiseases[tumor]" name="otherdiseases[tumor]" value="<%= @otherdiseases.tumor %>" <%= placeholder( "Tumor" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="otherdiseases[gout]" class="fieldLabel">Gout</label>
            <input type="text" id="otherdiseases[gout]" name="otherdiseases[gout]" value="<%= @otherdiseases.gout %>" <%= placeholder( "Gout" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
