<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @heartblood.id %></h1>
    <a href="<%= url_for :action => :show, :id => @heartblood.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @heartblood.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @heartblood.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="heartblood[id]" class="fieldLabel">Id</label>
            <input type="text" id="heartblood[id]" name="heartblood[id]" value="<%= @heartblood.id %>" <%= placeholder( "Id" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[chestpain]" class="fieldLabel">Chestpain</label>
            <input type="text" id="heartblood[chestpain]" name="heartblood[chestpain]" value="<%= @heartblood.chestpain %>" <%= placeholder( "Chestpain" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[rheumaticfever]" class="fieldLabel">Rheumaticfever</label>
            <input type="text" id="heartblood[rheumaticfever]" name="heartblood[rheumaticfever]" value="<%= @heartblood.rheumaticfever %>" <%= placeholder( "Rheumaticfever" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[infraction]" class="fieldLabel">Infraction</label>
            <input type="text" id="heartblood[infraction]" name="heartblood[infraction]" value="<%= @heartblood.infraction %>" <%= placeholder( "Infraction" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[bp]" class="fieldLabel">Bp</label>
            <input type="text" id="heartblood[bp]" name="heartblood[bp]" value="<%= @heartblood.bp %>" <%= placeholder( "Bp" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[breathlessness]" class="fieldLabel">Breathlessness</label>
            <input type="text" id="heartblood[breathlessness]" name="heartblood[breathlessness]" value="<%= @heartblood.breathlessness %>" <%= placeholder( "Breathlessness" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[palpitation]" class="fieldLabel">Palpitation</label>
            <input type="text" id="heartblood[palpitation]" name="heartblood[palpitation]" value="<%= @heartblood.palpitation %>" <%= placeholder( "Palpitation" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="heartblood[heartattack]" class="fieldLabel">Heartattack</label>
            <input type="text" id="heartblood[heartattack]" name="heartblood[heartattack]" value="<%= @heartblood.heartattack %>" <%= placeholder( "Heartattack" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
