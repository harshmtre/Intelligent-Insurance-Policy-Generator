<div data-role="page">

	<div data-role="header" data-position="inline">
    <h1>Personal Form</h1>
    <a href="<%= url_for :controller => :Login, :action => :MainPage %>" class="ui-btn-left" data-icon="home" data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>>
      Home
    </a>
    <a href="<%= url_for :action => :new %>" class="ui-btn-right" data-icon="plus">
      New
    </a>
	</div>
  <div data-role="content">
    <ul data-role="listview">
      <% @personalforms.each do |personalform| %>
      
          <li>
            <a href="<%= url_for :action => :show, :id => personalform.object %>">
              <%= personalform.Id %>
            </a>
          </li>
      
      <% end %>
      
    </ul>
  </div>
<a href="Delete"> Delete ALL </a>
</div>
